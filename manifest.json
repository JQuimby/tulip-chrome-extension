{
  "manifest_version": 2,
  "name":"Tulip Changer",
  "version":"0.0.1",
  "description":"A joking perspective shift on NFTs.",
  "icons": { "16": "assets/icon16.png",
             "48": "assets/icon48.png",
            "128": "assets/icon128.png" },
  "author": "Justin Quimby",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/changeText.js"]
    }],
  "options_page": "pages/options.html",
  "homepage_url":"https://github.com/jquimby/tulip-chrome-extension",
  "permissions": [
    "storage"
  ],
  "browser_action": {},
  "background": {
    "scripts": ["scripts/onInstalled.js", "scripts/badge.js"],
    "persistent": false
  }
}
